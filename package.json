{
  "name": "remult-vue-todo",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently -k -n \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\" \"ts-node-dev -P tsconfig.server.json src/server/\" \"vite\"",
    "build": "run-p type-check build-only && tsc -p tsconfig.server.json",
    "start": "node dist/server/",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "heroku-ssl-redirect": "^0.1.1",
    "pg": "^8.8.0",
    "remult": "^0.16.0-exp.19",
    "vue": "^3.2.38"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.56",
    "@types/pg": "^8.6.5",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.6"
  }
}
